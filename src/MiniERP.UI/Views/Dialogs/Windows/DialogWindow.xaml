<Window x:Class="MiniERP.UI.Views.Dialogs.Windows.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiniERP.UI.Views.Dialogs.Windows"
        mc:Ignorable="d"
        Title="{Binding Title}" 
        mvvm:ViewModelLocator.AutoWireViewModel="True"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:mvvm="http://prismlibrary.com/"
        AllowsTransparency="False"
        WindowStyle="SingleBorderWindow"
        Background="{x:Null}"
        TextElement.Foreground="{DynamicResource MaterialDesignPaper}"
        materialDesign:ScrollBarAssist.ButtonsVisibility="Visible"
        WindowStartupLocation="CenterScreen" x:ClassModifier="internal">

</Window>
